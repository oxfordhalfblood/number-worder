#!/usr/bin/env python3

import argparse
import sys
from converter import Converter, Parameter

def main():
    converter = Converter()
    param = Parameter()
    # parsed = param.parse_args(converter)
    parsed = param.parsefunc(sys.argv[1:])
    print("parsed", parsed.num)
    # if the parser fails, app exits and the following is not executed
    converter.num_to_word(parsed.num)

if __name__=="__main__":
    main()